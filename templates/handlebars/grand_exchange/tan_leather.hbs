<a href="{{concat 'base_url' '/grand-exchange/tan-leather'}}"><h2>Tan Leather</h2></a>

<table id="table-container">
    <thead>
        <tr>
            <th scope="col">Hide</th>
            <th scope="col">Buy Price</th>
            <th scope="col">Al Kharid Cost</th>
            <th scope="col">Canifis Cost</th>
            <th scope="col">Cheapest Cost</th>
            <th scope="col">Leather</th>
            <th scope="col">Sell Price</th>
            <th scope="col">GE Limit</th>
            <th scope="col">Profit</th>
        </tr>
    </thead>
    <tbody>
    {{#each this}}
        <tr>
            <td> <a href="{{concat 'base_url' '/grand-exchange/' hide_id}}"> <img class="tippy" data-title="{{hide_name}}" src="{{concat 'base_url' '/templates/bundle/assets/rs_items/' hide_file_name}}" /> </a></td>
            <td>{{numCommaFormat hide_cost}}{{#if hide_updated_time}}<a id="question" class="tippy" data-title="The last known result was updated {{timePassed hide_updated_time}} ago.">?</a>{{/if}}</td>
            <td>{{numCommaFormat al_kharid_cost}}</td>
            <td>{{numCommaFormat canifis_cost}}</td>
            <td>{{numCommaFormat total_cost}}</td>
            <td> <a href="{{concat 'base_url' '/grand-exchange/' leather_id}}"> <img class="tippy" data-title="{{leather_name}}" src="{{concat 'base_url' '/templates/bundle/assets/rs_items/' leather_file_name}}" /> </a></td>
            <td>{{numCommaFormat leather_sale}}{{#if leather_updated_time}}<a id="question" class="tippy" data-title="The last known result was updated {{timePassed leather_updated_time}} ago.">?</a>{{/if}}</td>
            <td>{{numCommaFormat limit}}</td>
            <td class="{{#ifCond profit '>' 0}}positive{{/ifCond}}{{#ifCond profit '<' 0}}negative{{/ifCond}}">{{#ifCond profit '>' 0}}+{{/ifCond}} {{numCommaFormat profit}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>