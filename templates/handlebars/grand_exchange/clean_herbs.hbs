<a href="{{concat 'base_url' '/grand-exchange/clean-herbs'}}"><h2>Clean Herbs</h2></a>

<table id="table-container">
    <thead>
        <tr>
            <th scope="col">Reqs.</th>
            <th scope="col">Item</th>
            <th scope="col">Grimy</th>
            <th scope="col">Buy Price</th>
            <th scope="col">Clean</th>
            <th scope="col">Sell Price</th>
            <th scope="col">GE Limit</th>
            <th scope="col">Profit</th>
        </tr>
    </thead>
    <tbody>
    {{#each this}}
        <tr>
            <td>{{requirement}}</td>
            <td>{{name}}</td>
            <td> <a href="{{concat 'base_url' '/grand-exchange/' grimy_id}}"> <img class="tippy" data-title="{{grimy_name}}" src="{{concat 'base_url' '/templates/bundle/assets/rs_items/' grimy_file_name}}" /> </a></td>
            <td>{{numCommaFormat grimy_cost}}{{#if grimy_updated_time}}<a id="question" class="tippy" data-title="The last known result was updated {{timePassed grimy_updated_time}} ago.">?</a>{{/if}}</td>
            <td> <a href="{{concat 'base_url' '/grand-exchange/' clean_id}}"> <img class="tippy" data-title="{{name}}" src="{{concat 'base_url' '/templates/bundle/assets/rs_items/' clean_file_name}}" /> </a></td>
            <td>{{numCommaFormat clean_sale}}{{#if clean_updated_time}}<a id="question" class="tippy" data-title="The last known result was updated {{timePassed clean_updated_time}} ago.">?</a>{{/if}}</td>
            <td>{{numCommaFormat grimy_buy_limit}}</td>
            <td class="{{#ifCond profit '>' 0}}positive{{/ifCond}}{{#ifCond profit '<' 0}}negative{{/ifCond}}">{{#ifCond profit '>' 0}}+{{/ifCond}} {{numCommaFormat profit}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>